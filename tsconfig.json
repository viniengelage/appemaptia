{
  "extends": "expo/tsconfig.base",
  "include": ["src"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "styles/*": ["./global/styles/*"],
      "screens/*": ["./screens/*"],
      "services/*": ["./services/*"],
      "utils/*": ["./utils/*"],
      "hooks/*":["./hooks/*"]
    }
  }
}
